{
    "schemaVersion": 1,
    "id": "${mod_id}",
    "name": "${mod_name}",
    "version": "${version}",
    "environment": "${environment}",
    "license": "${license}",
    "description": "${description}",
    "icon": "assets/${mod_id}/icon.png",
    "authors": [
        "${mod_author}"
    ],
    "contact": {
        "homepage": "${homepage_url}",
        "sources": "${sources_url}",
        "issues": "${issues_url}"
    },
    "accessWidener": "${mod_id}.accesswidener",
    "entrypoints": {
        "client": [
            "notryken.chatnotify.ChatNotifyFabric"
        ],
        "modmenu": [
            "notryken.chatnotify.gui.screen.ModMenuIntegration"
        ]
    },
    "mixins": [
        "${mod_id}.mixins.json",
        "${mod_id}.fabric.mixins.json"
    ],
    "depends": {
        "fabricloader": ">=0.14",
        "fabric-api": "*",
        "minecraft": ">=${minecraft_version}",
        "java": ">=17"
    },
    "recommends": {
        "modmenu": "${modmenu_version}"
    },
    "custom": {
        "modmenu": {
            "links": {
                "Contact": "https://linktr.ee/notryken"
            }
        }
    }
}
  