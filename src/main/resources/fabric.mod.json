{
  "schemaVersion": 1,
  "id": "chat-notify",
  "version": "${version}",
  "name": "Chat Notify",
  "description": "A client-side Fabric mod that allows you to set up audio and visual notifications for when specified words or phrases appear in chat.",
  "authors": [
    "NotRyken"
  ],
  "contact": {
    "homepage": "https://modrinth.com/mod/chat-notify",
    "sources": "https://github.com/NotRyken/ChatNotify",
    "issues": "https://github.com/NotRyken/ChatNotify/issues"
  },
  "license": "MIT",
  "icon": "assets/chatnotify/icon.png",
  "environment": "client",
  "entrypoints": {
    "client": [
      "notryken.chatnotify.client.ChatNotifyClient"
    ],
    "modmenu": [
      "notryken.chatnotify.gui.screen.ModMenuIntegration"
    ]
  },
  "mixins": [
    "chatnotify.mixins.json"
  ],
  "accessWidener": "chatnotify.accesswidener",
  "depends": {
    "fabricloader": ">=0.14.0",
    "fabric": "*",
    "minecraft": "1.20.2"
  }
}
